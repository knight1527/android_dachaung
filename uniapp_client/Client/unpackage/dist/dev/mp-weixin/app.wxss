	border-radius: 20rpx;
}

	/* 组件大小 */
.fill{
	height: 100%;
	width: 100%;
}
.fill_width{
	width: 100%;
}
.fill_height{
	height: 100%;
}

	/* 文章相关信息 */
.sec_message{
	font-size: 24rpx;
	color: grey;
	margin-right: 20rpx;
}
.sec_avatar{
	height: 50rpx;
	width: 50rpx;
	border-radius: 50rpx;
}
.white-space{
	overflow:hidden;
}

	/* 布局 */
.aligin_center_item_v{
		display: flex;
		flex-direction: column;
		text-align: center;
		justify-content: center;
		align-items: center;
}
.position_static{
	position: static;
}
.overflowHide{
	overflow: hidden;
}

	/* 赛事卡片 */
.absolute_event_status{
	position: absolute;
	top: 0rpx;
	right: 0rpx;
	z-index: 1;
	font-weight: 500rpx;
	font-size: 30rpx;
	letter-spacing: 8rpx;
}
.status_before{
	color: white;
	background-color: #03DAC5;
}
.status_ing{
	color: white;
	background-color: #87CEEB;
}
.status_end{
	color: white;
	background-color: darkgrey;
}
.eventTitle{
	font-size: 36rpx;
	font-weight: 500rpx;
	letter-spacing: 4rpx;
	overflow: hidden;
}
.eventTime{
	font-size: 28rpx;
	color: dimgrey;
	font-weight: 400rpx;
}
.event_other{
	font-size: 24rpx;
	color: grey;
	font-weight: 400rpx;
}

	/* 时间 */
.time{
	font-size: 25rpx;
	color: #808080;
}

	/* 文章标题 */
.sec_title{
	font-size: 50rpx;
	font-weight: 400;
}

	/* 标签 */
.tag{
	font-size: 30rpx;
	background-color: #03DAC5;
	color: white;
	border-radius: 10rpx;
	padding: 10rpx;
}

